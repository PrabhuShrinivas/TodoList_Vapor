<html>
  <head>
    <title>Todo List</title>
  </head>
  <body>
    <h1>Todo List</h1>
    <ul>
      #for(todo in todos) {
        <li>
          #if(todo.completed) {
            <strike>#(todo.title)</strike>
          } else {
            #(todo.title)
          }
        </li>
      }
    </ul>
  </body>
</html>
